#Errores de tipo en instrucciones
program {
    using
        int i,j,k;
        set s;
        bool b;
    in

    i = 5+j*(k*k/2);

    for i max b do
        if (s) print i else print j;

    repeat
        for j min b<+>s do {
            using
                int x;
            in
            if (b) scan x else print x;
        }
    while (i) do
        print "Intenseo";
}

