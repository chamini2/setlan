# Caso con definicion de conjuntos con operaciones internas.
# Este caso debe de correr.
program {
   using
       int i,j,k;
       set s;
   in

   i = 1;
   j = 1;
   k = 2;

   s = {};
   s = {i,j,k,j+k,i/k*2};

   println "Fumanacci", s;
}

